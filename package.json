    "lodash": "^4.17.11",
    "lodash.chunk": "^4.2.0",
    "lodash.throttle": "^4.1.1",
    "lodash.uniq": "^4.5.0",
    "lodash.uniqby": "^4.7.0",
    "metaget": "^1.0.6",
    "react": "^16.9.0",
    "react-dom": "^16.5.2",
    "react-helmet": "^5.2.0",
    "react-markdown": "^4.0.8",
    "react-responsive": "^7.0.0",
    "react-scroll-parallax": "^2.1.2",
    "react-spring": "^8.0.27",
    "react-ticker": "^1.2.0",
